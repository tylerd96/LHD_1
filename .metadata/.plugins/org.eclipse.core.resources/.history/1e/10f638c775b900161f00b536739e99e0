
public enum spriteRdr {
	PACMAN(1,1),
	BLINKY(2,2),
	INKY(3,3),
	CLYDE(4,4),
	PINKY(5,5);
	
	final int size = 24;
	int x,y;
	
	spriteRdr(int x, int y) {
		setX(x*size);
		setY(y*size);
	}
	
	public int getX() {return x;}
	public void setX(int x) {this.x = x;}
	public int getY() {return y;}
	public void setY(int y) {this.y = y;}
}
